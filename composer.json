{
    "name": "spojenet/cnb-exchange-rate-list2json",
    "description": "Application for downloading the CNB exchange rate list.",
    "type": "project",
    "require": {
        "vitexsoftware/ease-core": "^1.49",
        "robmorgan/phinx": "^0.16.6",
        "vitexsoftware/ease-fluentpdo": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "SpojeNet\\Cnb\\": "src/SpojeNet/Cnb"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.91",
        "ergebnis/composer-normalize": "^2.48",
        "ergebnis/php-cs-fixer-config": "^6.58"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }

}
